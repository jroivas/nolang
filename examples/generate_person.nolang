pure generatePerson(name:String, birthDay:Date) : Map =>
    // Just plain list
    [1, 2, 3, 4, 8]
    // Map
    [
        "name": name,
        "birthDate": birthDay
    ]
    // Same as:
    [("name", name), ("birthDate", birthDay)]
    // Same as:
    mapSet("birthDate", birthDay)
    mapSet(mapSet([], "name", name), "birthDate", birthDay)

